@model List<MidTermPracrice.Models.Interpolate>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Interpolation</h1>

<a class="btn btn-primary" asp-action="create">Create new numbers</a>

<br /><br />

<table id="myTable" class="table table-bordered" cellspacing="0">
        <thead class="thead-dark">
            <tr>
                @foreach (var property in Model.GetType().GetGenericArguments()[0].GetProperties())
                {
                    <th>@property.Name</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                @foreach (var property in item.GetType().GetProperties())
                {
                     <td>@property.GetValue(item)</td>
                }
                </tr>
            }
        </tbody>
</table>